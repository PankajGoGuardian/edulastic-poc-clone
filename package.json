{
  "name": "edulastic",
  "version": "8.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "nodemonConfig": {
    "ignore": [
      "**node_modules**",
      ".build/**",
      "coverage/**",
      "src/client/**"
    ]
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/client/**/*.js' !src/client/public/**/*.js",
    "format-check": "prettier --check 'src/client/**/*.js' !src/client/public/**/*.js",
    "execute": "ts-node --compiler-options '{react-outside-event\"lib\":[\"ESNext\"]}' ",
    "build": "node --max-old-space-size=4096 node_modules/poi/bin/cli.js --prod --parallel",
    "start": "APP_MODE_DEV=1 poi --serve --open",
    "doc:start": "DOC_MODE=1 APP_MODE_DEV=1 poi src/doc/index.html --serve --open",
    "prod-serve": "serve -s dist -p 3001",
    "test": "jest --coverage && ava",
    "jest": "jest",
    "ava": "ava --watch",
    "test:watch": "jest --watch",
    "test:automation-open": "cypress open",
    "test:automation-run": "cypress run",
    "test:automation-run-poc": "cypress run --env configFile=poc",
    "test:automation-run-uat": "cypress run --env configFile=uat",
    "test:automation-run-prod": "cypress run --env configFile=prod",
    "test:automation-run-qa": "cypress run --env configFile=qa",
    "test:automation-open-poc": "cypress open --env configFile=poc",
    "test:automation-open-uat": "cypress open --env configFile=uat",
    "test:automation-open-prod": "cypress open --env configFile=prod",
    "test:automation-open-qa": "cypress open --env configFile=qa",
    "test:visual-regression-open": "cypress open --env configFile=visual-regression,updateSnapshots=true",
    "test:visual-regression-run": "cypress run --env configFile=visual-regression,updateSnapshots=false",
    "test:visual-regression-run:update-base": "cypress run --env configFile=visual-regression,updateSnapshots=true",
    "cy:clear-reports": "rm -rf cypress/reports cypress/results",
    "cy:merge-reports": "mochawesome-merge --reportDir cypress/reports > cypress/reports/cypress-report.json",
    "cy:merge-reports-docker": "mochawesome-merge --reportDir docker_results/json > cypress/reports/cypress-report.json",
    "cy:generate-report": "marge --no-code --cdn=true -o=cypress/reports -f=Cypress_Test_Report --ts=fullDate cypress/reports/cypress-report.json",
    "publish-evaluators": "git subtree push --prefix packages/evaluators git@github.com:snapwiz/edu-eval.git master",
    "publish-constants": "git subtree push --prefix packages/constants git@github.com:snapwiz/edu-constants.git master",
    "storybook": "STORYBOOK_ENABLED=1 NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
