{
  "name": "edulastic",
  "version": "41.2.0",
  "sideEffects": [
    "!(**/ducks.js)",
    "!(**/duck.js)",
    "!(**/sharedDucks/**)",
    "!(**/sagas/**)",
    "!(**/reducers/**)",
    "!(**/sagas.js)",
    "!(**/loginSagas.js)",
    "!(**/reducers.js)",
    "!(**/src/(sagas|selectors|reducers|utils|styles|actions)/**)",
    "!(**/utils/**)",
    "!(**/common/**)"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "nodemonConfig": {
    "ignore": [
      "**node_modules**",
      ".build/**",
      "coverage/**",
      "src/client/**"
    ]
  },
  "scripts": {
    "bump": "./bump.sh",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/client/**/*.js' !src/client/public/**/*.js",
    "format-check": "prettier --check 'src/client/**/*.js' !src/client/public/**/*.js",
    "execute": "ts-node --compiler-options '{react-outside-event\"lib\":[\"ESNext\"]}' ",
    "dev:extension": "webpack-dev-server --open --config src/chrome-extension/webpack.config.js",
    "build:extension": "webpack --config src/chrome-extension/webpack.config.js",
    "start": "APP_MODE_DEV=1 react-app-rewired start",
    "start:test": "CYPRESS=true APP_MODE_DEV=1 react-app-rewired -r @cypress/instrument-cra start",
    "build": "NODE_ENV=production react-app-rewired build",
    "buildwlimit": "NODE_ENV=production react-app-rewired --max_old_space_size=6096 build",
    "build:login2": "webpack --config src/webpack.login.config.js",
    "prod-serve": "serve -s build -p 3001",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "test:silent": "react-scripts test --silent --watchAll=false",
    "ava": "ava --watch",
    "publish-constants": "git subtree push --prefix packages/constants git@github.com:snapwiz/edu-constants.git master",
    "storybook": "STORYBOOK_ENABLED=1 NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier:path": "yarn prettier --write",
    "prettier:check": "yarn prettier --check \"src/**/*.js|src/**/*.ts\"",
    "prettier:format": "yarn prettier --write \"src/**/*.js|src/**/*.ts\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@edulastic/evaluators": "git+ssh://git@github.com/snapwiz/edu-eval.git#3d369a0d95132fa854fa79bb13196052bf8255da",
