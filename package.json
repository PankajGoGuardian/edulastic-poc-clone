    "@vx/text": "^0.0.183",
    "@welldone-software/why-did-you-render": "^4.3.2",
    "antd": "^3.26.14",
    "axios": "^0.21.1",
    "brace": "^0.11.1",
    "canvas": "^2.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "color": "^3.1.0",
    "connected-react-router": "^4.5.0",
    "console.table": "^0.10.0",
