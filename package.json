{
  "name": "edulastic",
  "version": "12.60.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "nodemonConfig": {
    "ignore": [
      "**node_modules**",
      ".build/**",
      "coverage/**",
      "src/client/**"
    ]
  },
  "scripts": {
    "bump": "./bump.sh",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/client/**/*.js' !src/client/public/**/*.js",
    "format-check": "prettier --check 'src/client/**/*.js' !src/client/public/**/*.js",
    "execute": "ts-node --compiler-options '{react-outside-event\"lib\":[\"ESNext\"]}' ",
    "dev:extension": "webpack-dev-server --open --config src/chrome-extension/webpack.config.js",
    "build:extension": "webpack --config src/chrome-extension/webpack.config.js",
    "start": "APP_MODE_DEV=1 react-app-rewired start",
    "start:test": "APP_MODE_DEV=1 react-app-rewired -r @cypress/instrument-cra start",
    "build": "NODE_ENV=production react-app-rewired build",
    "buildwlimit": "NODE_ENV=production react-app-rewired --max_old_space_size=4096 build",
    "prod-serve": "serve -s build -p 3001",
    "test": "jest --coverage && ava",
    "jest": "jest",
    "ava": "ava --watch",
    "test:watch": "jest --watch",
    "test:automation-open": "cypress open",
    "test:automation-run": "cypress run",
    "test:automation-run-poc": "cypress run --env configFile=poc",
    "test:automation-run-uat": "cypress run --env configFile=uat",
    "test:automation-run-prod": "cypress run --env configFile=prod",
    "test:automation-run-qa": "cypress run --env configFile=qa",
    "test:automation-open-poc": "cypress open --env configFile=poc",
    "test:automation-open-uat": "cypress open --env configFile=uat",
    "test:automation-open-prod": "cypress open --env configFile=prod",
    "test:automation-open-qa": "cypress open --env configFile=qa",
    "test:visual-regression-open": "cypress open --env configFile=visual-regression,updateSnapshots=true",
    "test:visual-regression-run": "cypress run --env configFile=visual-regression,updateSnapshots=false",
    "test:visual-regression-run:update-base": "cypress run --env configFile=visual-regression,updateSnapshots=true",
    "cy:clear-reports": "rm -rf cypress/reports cypress/results",
    "cy:merge-reports": "mochawesome-merge --reportDir cypress/reports > cypress/reports/cypress-report.json",
    "cy:merge-reports-docker": "mochawesome-merge --reportDir docker_results/json > cypress/reports/cypress-report.json",
    "cy:generate-report": "marge --no-code --cdn=true -o=cypress/reports -f=Cypress_Test_Report --ts=fullDate cypress/reports/cypress-report.json",
    "publish-constants": "git subtree push --prefix packages/constants git@github.com:snapwiz/edu-constants.git master",
    "storybook": "STORYBOOK_ENABLED=1 NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier:path": "yarn prettier --write",
    "prettier:check": "yarn prettier --check \"src/**/*.js|src/**/*.ts\"",
    "prettier:format": "yarn prettier --write \"src/**/*.js|src/**/*.ts\""
  },
  "dependencies": {
    "@edulastic/evaluators": "git+ssh://git@github.com/snapwiz/edu-eval.git#23a0a026c6fd9bfc2afc2f3dd988d78920791316",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.13.2",
    "@sentry/browser": "^5.26.0",
    "@sentry/tracing": "^5.26.0",
    "@use-it/interval": "^0.1.3",
    "@vx/text": "^0.0.183",
    "@welldone-software/why-did-you-render": "^4.3.2",
    "antd": "^3.26.14",
    "axios": "^0.19.1",
    "brace": "^0.11.1",
    "canvas": "^2.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "color": "^3.1.0",
    "connected-react-router": "^4.5.0",
    "console.table": "^0.10.0",
    "core-js": "^3.1.3",
    "cypress": "4.12.1",
    "cypress-file-upload": "^3.5.3",
    "d3-array": "^2.0.3",
    "dotenv": "^8.2.0",
    "eslint-plugin-cypress": "^2.1.2",
    "faker": "^4.1.0",
    "fetch-mock": "^7.2.5",
    "firebase": "^7.13.2",
    "font-awesome": "^4.7.0",
    "froala-editor": "3.1.1",
    "history": "^4.7.2",
    "howler": "^2.2.0",
    "jquery": "^3.5.1",
    "jsxgraph": "^0.99.7",
    "keyboardevent-key-polyfill": "^1.1.0",
    "loadjs": "^4.2.0",
    "lodash": "^4.17.11",
    "mathjs": "^5.8.0",
    "mathquill": "^0.10.1-a",
    "memoize-one": "^5.0.4",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "3.1.1",
    "mochawesome-merge": "1.0.7",
    "mochawesome-report-generator": "3.1.5",
    "mqtt": "^2.18.8",
    "nanoid": "^2.1.10",
    "normalizr": "^3.3.0",
    "overlay-pslg": "^1.0.0",
    "prop-types": "^15.6.2",
    "qs": "^6.7.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.13.1",
    "react-ace": "^8.0.0",
    "react-autosize-textarea": "4.0.0",
    "react-click-outside": "^3.0.1",
    "react-color": "^2.17.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-csv": "^1.1.1",
    "react-dev-utils": "^5.0.2",
    "react-dnd": "^9.3.4",
    "react-dnd-html5-backend": "^9.3.4",
    "react-dnd-touch-backend": "^9.4.0",
    "react-dom": "^16.13.1",
    "react-drag-and-drop": "^2.4.0",
    "react-dropzone": "^10.1.4",
    "react-froala-wysiwyg": "3.1.1",
    "react-google-login": "^5.0.2",
    "react-icons": "^3.1.0",
    "react-joyride": "^2.0.5",
    "react-jsx-parser": "1.18.0",
    "react-media-player": "^0.7.7",
    "react-notifications": "^1.4.3",
    "react-outside-event": "^1.2.7",
    "react-pdf": "^4.1.0",
    "react-perfect-scrollbar": "^1.4.2",
    "react-quill": "^1.3.3",
    "react-redux": "^5.1.1",
    "react-responsive-modal": "^3.5.1",
    "react-rnd": "^9.1.1",
    "react-rnd-rotate": "^0.0.19",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-sortable-hoc": "^0.8.3",
    "react-stripe-elements": "^6.0.1",
    "recharts": "^1.4.2",
    "redux": "^4.0.1",
    "redux-freeze": "^0.1.7",
    "redux-ignore": "^1.2.5",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.10.0",
    "redux-reset": "^0.3.0",
    "redux-saga": "^0.16.2",
    "redux-starter-kit": "^0.5.1",
    "redux-undo": "^0.6.1",
    "reselect": "^3.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "striptags": "^3.1.1",
    "styled-components": "^5.1.0",
    "timm": "^1.7.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "11.0.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/react": "^16.9.55",
    "ava": "1.0.0-rc.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-react-require": "^3.1.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "customize-cra": "1.0.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-promise": "^1.0.2",
    "cypress-testing-library": "^2.2.1",
    "cypress-xpath": "^1.3.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.86.0",
    "husky": "^4.3.0",
    "jest-cli": "^23.1.0",
    "jira-prepare-commit-msg": "^1.4.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.5.0",
    "mocha": "5.2.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "pretty-quick": "^3.0.2",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "4.0.0",
    "redux-saga-test-plan": "^3.7.0",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "serve": "^10.1.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "socket.io-client": "^2.3.0",
    "test-data-bot": "^0.6.0",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "ava": {
    "files": [
      "src/server/tests/**/*.js"
    ],
    "sources": [
      "src/server/**/*.js"
    ],
    "cache": true,
    "verbose": true,
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    }
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$M [$J]",
    "jiraTicketPattern": "(EV+-\\d+)",
    "isConventionalCommit": true
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,css,scss}": [
      "prettier --write"
    ]
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "setupTestFrameworkScriptFile": "<rootDir>/config/test-setup.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest-transform.js"
    },
    "testRegex": "__test__/.*\\.test\\.js$",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/styleMock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
