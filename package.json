    "react-app-rewired": "^2.1.6",
    "react-scripts": "4.0.0",
    "redux-saga-test-plan": "^3.7.0",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "git+ssh://git@github.com/aga5tya/script-ext-html-webpack-plugin.git#5784e3147996fe412c070a8b534e10daafb5bab3",
    "serve": "^10.1.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "socket.io-client": "^2.3.0",
    "test-data-bot": "^0.6.0",
    "webpack-ant-icon-loader": "^1.0.8",
    "webpack-bundle-analyzer": "^3.9.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "ava": {
    "files": [
      "src/server/tests/**/*.js"
    ],
    "sources": [
      "src/server/**/*.js"
    ],
    "cache": true,
    "verbose": true,
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    }
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$M [$J]",
    "jiraTicketPattern": "(EV+-\\d+)",
    "isConventionalCommit": true
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,css,scss}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec).[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!((antd|rc-util|rc-pagination|rc-calendar|react-redux)/es|@edulastic|react-dnd|dnd-core|react-dnd-touch-backend|react-dnd-html5-backend|@babel/runtime)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|pdf)$": "<rootDir>/config/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/config/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}",
      "!src/index.js",
      "!src/login.js",
      "!src/service-worker*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
